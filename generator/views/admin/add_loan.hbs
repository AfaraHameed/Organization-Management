<section>
    <div class="container col-md-6 mt-5">

        <form action="add_loan" method="post" class="row g-3">
    {{#each member}}      
  <div class="col-md-6">
    <label for="inputEmail4" class="form-label">Reg Id</label>
    <input type="text" class="form-control" id="inputRegId" name="RegId" value="{{this.regId}}" readonly>
  </div>
 
  <div class="col-md-6">
    <label for="inputEmail4" class="form-label">name</label>
    <input type="text" class="form-control" id="inputName" name="name" value="{{this.userName}}" readonly>
  </div>
  <div class="col-md-6">
    <label for="inputCity" class="form-label">email</label>
    <input type="text" class="form-control" id="inputemail" name="email" value="{{this.email}}" readonly>
  </div>
   <div class="col-md-6">
    <label for="inputCity" class="form-label">Amount</label>
    <input type="text" class="form-control" id="inputAmount" name="amount">
  </div>
   <div class="form-group"> <!-- Date input -->
        <label class="control-label" for="date">Date</label>
        <input class="form-control" id="date" name="date" placeholder="MM/DD/YYY" type="date"/>
    </div>
  
 
  <div class="col-12">
    <button type="submit" class="btn btn-primary">Sumbit</button>
  </div>
  {{/each}}
</form>

    </div>
</section>