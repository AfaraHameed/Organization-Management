<section>

    
    
    <div class="container">
        <table class="table table-hover">
        <thead>
        <tr>
        <th scope="col">Reg Id</th>
        <th scope="col">Name</th>
        <th scope="col">Email</th>
        <th scope="col">Amount</th>
        <th scope="col">Date</th>
        
        </tr>
        </thead>
        <tbody>
            {{#each installment}}
            <tr>
            <th scope="row">{{this.RegId}}</th>
            <td>{{this.name}}</td>
            <td>{{this.email}}</td>
            <td>{{this.amount}}</td>
            <td>{{this.date}}</td>
           
            </tr>
            {{/each}}
           
        </tbody>
        </table>
    </div>
</section>